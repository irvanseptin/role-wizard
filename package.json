{
  "name": "role-wizard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "api1": "json-server --watch db-step1.json --port 4001",
    "api2": "json-server --watch db-step2.json --port 4002",
    "dev": "concurrently \"npm run api1\" \"npm run api2\" \"next dev\"",
    "api:reset": "npm run api:reset1 && npm run api:reset2"
  },
  "dependencies": {
    "json-server": "^1.0.0-beta.3",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "sass": "^1.96.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "typescript": "^5"
  }
}
